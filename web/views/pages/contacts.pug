extends ../layout.pug

block content
    .row.mt-4
        .col-12
            .card
                .card-header Contacts
                .card-body
                    form.col-12.mb-4(action="")
                        .input-group
                            input.form-control(placeholder="Search...")
                            .input-group-append
                                .input-group-text.bg-light
                                        span.fas.fa-search
                    .row
                        .card.p-0.col-4.col-xl-3
                            .card-header Dylan Dunn
                                a.float-right.btn.btn-sm.btn-danger
                                    span.fas.fa-times
                                a.float-right.btn.btn-sm.btn-primary.mr-1
                                    span.fas.fa-edit
                            img.card-img.top(height="200x" src="https://www.telegraph.co.uk/content/dam/films/2017/08/17/fp_trans_NvBQzQNjv4BqCceMXJtaZGMG8GkK7K3ZQhj5UbS2BMzlXVPsqEE56Qo.png?imwidth=450")
                            .card-body.pb-0.text-center
                                table.table.table-borderless.table-light
                                    tr.text-left
                                        th.text-right Phone
                                        td 555-565-7098
                                    tr.text-left
                                        th.text-right Email
                                        td robodylan123@gmail.com
                                    tr.text-left
                                        th.text-right Birthday
                                        td 01/21/2000
                                .btn-group.mb-4
                                    a.btn.btn-secondary
                                        span.fab.fa-discord
                                    a.btn.btn-secondary
                                        span.fab.fa-twitter
                                    a.btn.btn-secondary
                                        span.fab.fa-instagram
                                    a.btn.btn-secondary
                                        span.fab.fa-facebook
                                    a.btn.btn-secondary
                                        span.fab.fa-snapchat-ghost
                                    a.btn.btn-secondary
                                        span.fab.fa-linkedin
                                    a.btn.btn-secondary
                                        span.fab.fa-github
                        .card.p-0.col-4.col-xl-3
                            .card-header New Contact
                                button.float-right.btn.btn-sm.btn-active(disabled)
                                    span.fas.fa-times
                                button.float-right.btn.btn-sm.btn-active.mr-1(disabled)
                                    span.fas.fa-edit
                            img.card-img.top(height="200px" src="data:image/png;base64,")
                            .card-body
                                form(action="")
                                    .form-row.mb-2
                                        .col-6
                                            input.form-control(type="text" name="first_name" placeholder="First Name")
                                        .col-6
                                            input.form-control(type="text" name="last_name" placeholder="Last Name")
                                    .form-row.mb-2
                                        .col-12
                                            .input-group
                                                .input-group-prepend
                                                    span.input-group-text.fas.fa-phone
                                                input.form-control(type="text" name="phone" placeholder="Phone")
                                    .form-row.mb-2
                                        .col-12
                                            .input-group
                                                .input-group-prepend
                                                    span.input-group-text @
                                                input.form-control(type="email" name="email" placeholder="Email")
                                    .form-row.mb-4
                                        .col-12
                                            .input-group.date
                                                .input-group-prepend
                                                    span.input-group-text.fas.fa-calendar-day
                                                input.form-control(name="due_date" type="date" value=new Date().toISOString().substr(0, 10))
                                    .form-row.mb-2
                                        .col-12
                                            .input-group.date
                                                .input-group-prepend
                                                    span.input-group-text.fab.fa-twitter
                                                input.form-control(type="twitter" name="twitter" placeholder="Twitter")
                                    .form-row.mb-2
                                        .col-12
                                            .input-group.date
                                                .input-group-prepend
                                                    span.input-group-text.fab.fa-instagram
                                                input.form-control(type="instagram" name="instagram" placeholder="Instagram")
                                    .form-row.mb-2
                                        .col-12
                                            .input-group.date
                                                .input-group-prepend
                                                    span.input-group-text.fab.fa-facebook
                                                input.form-control(type="facebook" name="facebook" placeholder="Facebook")
                                    .form-row.mb-2
                                        .col-12
                                            .input-group.date
                                                .input-group-prepend
                                                    span.input-group-text.fab.fa-snapchat
                                                input.form-control(type="snapchat" name="snapchat" placeholder="Snapchat")
                                    .form-row.mb-2
                                        .col-12
                                            .input-group.date
                                                .input-group-prepend
                                                    span.input-group-text.fab.fa-linkedin
                                                input.form-control(type="linkedin" name="linkedin" placeholder="LinkedIn")
                                    .form-row.mb-2
                                        .col-12
                                            .input-group.date
                                                .input-group-prepend
                                                    span.input-group-text.fab.fa-github
                                                input.form-control(type="github" name="github" placeholder="Github")
                                    
                                        



                                